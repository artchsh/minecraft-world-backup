# Example .env for Minecraft World Backup
SOURCE_FOLDER=C:/path/to/minecraft/world
LOCAL_BACKUP_ENABLED=true
LOCAL_BACKUP_FOLDER=C:/path/to/local/backups
LOCAL_BACKUP_MAX=10
LOCAL_BACKUP_DELAY_MINUTES=60
FTP_BACKUP_ENABLED=true
FTP_HOST=ftp.example.com
FTP_PORT=21
FTP_USERNAME=your_username
FTP_PASSWORD=your_password
FTP_FOLDER=/backups/minecraft
FTP_BACKUP_MAX=10
FTP_BACKUP_DELAY_MINUTES=60
